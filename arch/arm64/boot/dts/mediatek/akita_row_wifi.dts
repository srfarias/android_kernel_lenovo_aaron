&accdet {
	accdet-mic-vol = <6>;
	headset-mode-setting = <0x500 0x500 1 0x1f0 0x800 0x800 0x20 0x44>;
	accdet-plugout-debounce = <1>;
	accdet-mic-mode = <1>;
	headset-eint-level-pol = <8>;
	headset-three-key-threshold = <0 80 220 400>;
	headset-three-key-threshold-CDD = <0 121 192 600>;
	headset-four-key-threshold = <0 58 121 192 400>;
	pinctrl-names = "default", "state_eint_as_int";
	pinctrl-0 = <&accdet_pins_default>;
	pinctrl-1 = <&accdet_pins_eint_as_int>;
	status = "okay";
};
&pio {
	accdet_pins_default: accdetdefault {
	};
	accdet_pins_eint_as_int: accdeteint@0 {
		pins_cmd_dat {
			pinmux = <PINMUX_GPIO9__FUNC_GPIO9>;
			slew-rate = <0>;
			bias-disable;
		};
	};
};

&i2c5 {
	bq2560x@6B {
		compatible = "ti,bq25601";
		reg = <0x6B>;
	
		ti,bq2560x,usb-vlim = <4600>;
		ti,bq2560x,usb-ilim = <500>;
		ti,bq2560x,usb-vreg = <4400>;
		ti,bq2560x,usb-ichg = <500>;

		ti,bq2560x,precharge-current = <256>;
		ti,bq2560x,termination-current = <180>;
		ti,bq2560x,boost-voltage = <5150>;
		ti,bq2560x,boost-current = <1200>;
		ti,bq2560x,enable_term;
		ti,bq2560x,stat-pin-ctrl = <3>;
		ti,bq2560x,vac-ovp-threshold = <6500>;
		pinctrl-names = "default", "pogo_otg_on","pogo_otg_off";
		pinctrl-0 = <&charger_default>;
		pinctrl-1 = <&charger_pogo_otg_on>;
		pinctrl-2 = <&charger_pogo_otg_off>;

	};
	bias_i2c@3e {
		compatible = "mediatek,I2C_LCD_BIAS";
		reg = <0x3E>;
		status = "okay";
	};
	rt-swchg@53 {
                compatible = "richtek,swchg";
                reg = <0x53>;
                status = "okay";

                rt,intr_gpio_num = <2>; /* direct defined GPIO num */
                rt,intr_gpio = <&pio 2 0x0>; /* GPIO */
                rt9471 {
                        /*
                         * Charger name
                         * Main charger: primary_chg
                         * Slave charger: secondary_chg
                         */
                        charger_name = "primary_chg";
                        chg_alias_name = "rt9471_chg";
                        rm-slave-addr = /bits/ 8 <0x53>;
                        rm-name = "rt9471";
                        interrupt-names = "wdt", "chg_mivr", "vbus_gd",
                                          "bc12_done", "detach", "vac_ov",
                                          "otg_fault";
                        ichg = <1000000>;               /* uA */
                        aicr = <500000>;                /* uA */
                        mivr = <4400000>;               /* uV */
                        cv = <4350000>;                 /* uV */
                        ieoc = <200000>;                /* uA */
                        safe-tmr = <10>;                /* hour */
                        wdt = <40>;                     /* sec */
                        /*
                         * 0: MIVR register
                         * 1: MIVR = VBAT + 200mV
                         * 2: MIVR = VBAT + 250mV
                         * >= 3: MIVR = VBAT + 300mV
                         */
                        mivr-track = <0>;
                        en-te;
                        /* en-jeita; */
                        /* ceb-invert; */
                        /* dis-i2c-tout; */
                        en-qon-rst;
                        /* auto-aicr; */
                };
        };
};

&pio {
	charger_default: charger_default {
	};
	charger_pogo_otg_on: pogo_otg_on {
		pins_cmd_dat {
			pinmux = <PINMUX_GPIO169__FUNC_GPIO169>;
			slew-rate = <1>;
			output-high;
		};
	};
	charger_pogo_otg_off: pogo_otg_off {
		pins_cmd_dat {
			pinmux = <PINMUX_GPIO169__FUNC_GPIO169>;
			slew-rate = <1>;
			output-low;
		};
	};
};

